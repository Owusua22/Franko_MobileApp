{
  "expo": {
    "name": "FrankoTrading",
    "slug": "Franko",
    "version": "57.0.1",
    "orientation": "default",
    "icon": "./assets/frankologo.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "developmentClient": false,
    "splash": {
      "image": "./assets/frankologo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.poldark.mrfranky2",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/frankologo.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.poldark.mrfranky2",
      "compileSdkVersion": 35,
      "targetSdkVersion": 35,
      "buildToolsVersion": "35.0.0"
    },
    "web": {
      "favicon": "./assets/frankologo.jpg"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "eas": {
        "projectId": "a2536358-be00-46c2-b2a4-8e1449f1df3d"
      }
    },
    "owner": "franko_dev-2",
    "plugins": [
      "expo-asset",
      "expo-localization",
      "expo-updates"
    ]
  }
}
